#!/bin/sh
BEMENU_PRIMARY="#F1C40F"
BEMENU_BACKGROUND="#22252A"
BEMENU_TITLE_FOREGROUND="#000000"
bemenu-run -i -p ">:[ " --hp 7 --tb "$BEMENU_PRIMARY" --tf "$BEMENU_TITLE_FOREGROUND" --hf "$BEMENU_PRIMARY" --ab "$BEMENU_BACKGROUND" --fb "$BEMENU_BACKGROUND" --nb "$BEMENU_BACKGROUND" -c -l 5 -W 0.4 --bdr "$BEMENU_BACKGROUND" -B 10
